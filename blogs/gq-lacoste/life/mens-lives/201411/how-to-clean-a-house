            $('#header').before( el );

            el.click(function(){
                // Reset cookie
                if(categorizr.isTablet ){
                    $.cookie('X-UA-Device-force', 'tablet',  { expires: -1, path: '/', domain: 'gq-magazin.de', secure: false });
                }
                else if(categorizr.isMobile){
                    $.cookie('X-UA-Device-force', 'mobile',  { expires: -1, path: '/', domain: 'gq-magazin.de', secure: false });
                }
                var regex = new RegExp("^"+location.protocol+'//'+location.hostname, 'i');
                location.href = location.href.replace(regex, location.protocol+'//m.gq-magazin.de');
            });
            if(document.referrer.indexOf('.google.') != -1){
                $.cookie('X-UA-Device-force', 'desktop',  { path: '/', domain: 'gq-magazin.de', secure: false });
            }
        }
    }
}());
</script>
</body>
</html>