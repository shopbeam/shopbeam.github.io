<script type="text/javascript">
    $('#global_search').focusin(function() {
        if ('Suchen' == $(this).attr('value')) {
            $(this).attr('value', '');
        }
    });
    $('#global_search').focusout(function() {
        if ('' == $(this).attr('value')) {
            $(this).attr('value', 'Suchen');
        }
    });
</script>

