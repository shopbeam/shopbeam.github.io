                moreItemsURL: '/jscontent/fashionshow_finder/(season)/{0}/(offset)/{1}/(limit)/{2}/(runway)/0', // indicates the URL from where to load additional carousel items for the initially displayed fashion show; Note: {0} will be replaced with <i>defaultNodeId</i> id, {1} will be replaced with the offset, {2} will be replaced with the limit
                defaultNodeId: '216675', // indicates the eZ node ID for the initially displayed fashion show
                season: { urlTemplate: '/jscontent/fashionshow_finder/(season)/{0}/(offset)/{1}/(limit)/{2}/(runway)/0' }, // Note: {0} will be replaced with the season's id, {1} will be replaced with the offset, {2} will be replaced with the limit
                designer: { urlTemplate: '/jscontent/fashionshow_finder/(manufacturer)/{0}/(offset)/{1}/(limit)/{2}/(runway)/0' }, // Note: {0} will be replaced with the designer's id, {1} will be replaced with the offset, {2} will be replaced with the limit
                locations: { urlTemplate: '/jscontent/fashionshow_finder/(location)/{0}/(offset)/{1}/(limit)/{2}/(runway)/0' } // Note: {0} will be replaced with the location's id, {1} will be replaced with the offset, {2} will be replaced with the limit
            });
