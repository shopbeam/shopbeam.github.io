<script type="text/javascript">
if (window.adgroupid == undefined) {
    window.adgroupid = Math.round(Math.random() * 1000);
}

if (typeof adtAsync_isLoading === 'undefined') {
    var adtAsync_isLoading = false;
}

function initAdtech_3478399_UID( slotName ) {
    console.log('initAdtech_ ...');

    if ( typeof IvwSlotManager !== "undefined" ) {
        if ( !slotName ) {
            slotName = IvwSlotManager.getSlotNameByPlacementCode( 3478399 );
        }

        IvwSlotManager.slots.setStatus( slotName, IvwSlotManager.SLOT_STATUS.LOADING );
    }

    console.log('[inline js] Initializing slot \'' + slotName + '\'...');    

    if (typeof writeCapture === 'undefined') {
        if (adtAsync_isLoading === false) {
            adtAsync_isLoading = true;
            var wcScript = document.createElement('script');
            wcScript.setAttribute('type', 'text/javascript');
            wcScript.setAttribute('src', 'http://aka-cdn.adtech.de/rm/lib/writeCapture/writeCapture-1.0.5-nolib-min.js');
            document.getElementById('asyncPlaceholder_3478399_UID').appendChild(wcScript);
      }
      window.wcScriptInterval_3478399_UID = setTimeout(loadAdtech_3478399_UID, 100);
    } else {
        console.log('[inline js] Slot \'' + slotName + '\' was initialized...');
        loadAdtech_3478399_UID( slotName );
    }
}

function loadAdtech_3478399_UID( slotName ) {
    console.log('loadAdtech_ ...');

    if (typeof writeCapture !== 'undefined') {
        if(typeof window.wcScriptInterval_3478399 !== undefined){
            clearTimeout(window.wcScriptInterval_3478399_UID);
        }

        var currentDiv = jQuery('#asyncPlaceholder_3478399_UID');

        /* Prevent double loading */
        if( currentDiv.find('[id*=__document_write]').length > 0 ){
            if ('undefined' !== typeof(console) && console && console.log) {
                console.log('Ad reload skipped. Too fast!!');
            }
            return;
        }
        adtAsync_isLoading = false;

        currentDiv.css('min-height', currentDiv.height());

        if(typeof window.timeout_3478399 !== undefined){
            window.clearTimeout(window.timeout_3478399);
        }

        var currentWriteCapture = writeCapture;
        currentWriteCapture.writeOnGetElementById = true;
        var adTagString = '<script type="text/javascript" src="http://adserver.adtech.de/addyn/3.0/1147.1/3478399/0/225/ADTECH;loc=100;target=_blank;key=gqn_homepage;alias=GQ11_Home_SB;grp='+window.adgroupid+';misc='+new Date().getTime()+'"><\/script>';
        currentWriteCapture.html(
            '#asyncPlaceholder_3478399_UID',
            adTagString,
            {
                done: function( ) {
                    window.timeout_3478399 = window.setTimeout(function( ) {
                        currentDiv.css('min-height', '')
                    }, 1000);

                    if ( typeof IvwSlotManager !== "undefined" ) IvwSlotManager.init.resume();
                }
            }
        );
    } else {
        window.wcScriptInterval_3478399_UID = setTimeout(loadAdtech_3478399_UID, 100);
    }
}
</script>
