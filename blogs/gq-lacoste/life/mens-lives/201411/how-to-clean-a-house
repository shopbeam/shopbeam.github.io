    var domainCheck = new RegExp('^((a|v)[1-3]\.)?(www|test|dev|vm)\.gq-magazin\.de');
    if(domainCheck.test(location.hostname) === true){
        if(categorizr.isMobile || categorizr.isTablet ){
            var mobileSite = 'http://m.gq-magazin.de';
            var desktopSite = 'http://www.gq-magazin.de';
            var displayMsg = 'Zur Ansicht für Tablet-PC und iPad';
            
                        
            var redirectUrl = window.location.href;
            redirectUrl = redirectUrl.replace(desktopSite, mobileSite);
