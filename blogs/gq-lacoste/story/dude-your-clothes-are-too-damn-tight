  	    "image": ["http://media.gq.com/photos/5583c4d409f0bee564423d52/master/pass/style-blogs-the-gq-eye-cowbell-will-ferrell.jpg"],
  	    "thumbnailUrl": "http://media.gq.com/photos/5583c4d409f0bee564423d52/master/pass/style-blogs-the-gq-eye-cowbell-will-ferrell.jpg",
  	    "datePublished": "2014-08-20T15:30:00.000Z",
