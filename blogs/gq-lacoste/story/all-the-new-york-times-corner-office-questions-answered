  	    "@context": "http://schema.org",
  	    "@type": "NewsArticle",
  	    "headline": "Hey Corner Office, Get Off My Back!",
  	    "alternativeHeadline": "Answering All the Questions Posed by The New York Times\u0027 Specious Business Column, Corner Office",
  	    "url": "http://gq.com/story/all-the-new-york-times-corner-office-questions-answered",
  	    "image": ["http://media.gq.com/photos/561d629da7113fb922cd583d/master/pass/CEO-test-3x2.jpg"],
  	    "thumbnailUrl": "http://media.gq.com/photos/561d629da7113fb922cd583d/master/pass/CEO-test-3x2.jpg",
