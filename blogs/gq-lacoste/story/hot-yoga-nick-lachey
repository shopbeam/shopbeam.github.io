  	  {
  	    "@context": "http://schema.org",
  	    "@type": "NewsArticle",
  	    "headline": "Hot Yoga Is the Best Place to Meet Women (and Nick Lachey)",
  	    "alternativeHeadline": "",
  	    "url": "http://gq.com/story/hot-yoga-nick-lachey",
  	    "image": ["http://media.gq.com/photos/55bfda3067a063d336ffbd66/master/pass/nick-lachey-final[1].jpg"],
  	    "thumbnailUrl": "http://media.gq.com/photos/55bfda3067a063d336ffbd66/master/pass/nick-lachey-final[1].jpg",
  	    "datePublished": "2015-08-04T10:00:00.000Z",
