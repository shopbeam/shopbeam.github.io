          b.src = "//static.bam-x.com/tags/" + account + ".js";
          var a = document.getElementsByTagName("script")[0];
          a.parentNode.insertBefore(b, a);
      })(window, document, "gq");
    </script>
    <!-- end BAM-X jstag -->
  </body>
</html>
