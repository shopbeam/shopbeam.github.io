  	    "@context": "http://schema.org",
  	    "@type": "NewsArticle",
  	    "headline": "Dale Talde\u0027s Kimchi Fried Rice Will Blow Your Mind",
  	    "alternativeHeadline": "",
  	    "url": "http://gq.com/story/kimchi-fried-rice-recipe-dale-talde",
  	    "image": ["http://media.gq.com/photos/55f3084189c0d3e261b212c2/master/pass/dale-talde-1.jpg"],
  	    "thumbnailUrl": "http://media.gq.com/photos/55f3084189c0d3e261b212c2/master/pass/dale-talde-1.jpg",
  	    "datePublished": "2015-09-12T13:00:06.000Z",
