  	    "@context": "http://schema.org",
  	    "@type": "NewsArticle",
  	    "headline": "James Bond\u0027s 10 Most Stylish Gadgets",
  	    "alternativeHeadline": "",
  	    "url": "http://gq.com/story/best-james-bond-007-gadgets",
  	    "image": ["http://media.gq.com/photos/5632a4d7acca7b7f780e5a6c/master/pass/bond-gadets-3x2.jpg"],
  	    "thumbnailUrl": "http://media.gq.com/photos/5632a4d7acca7b7f780e5a6c/master/pass/bond-gadets-3x2.jpg",
