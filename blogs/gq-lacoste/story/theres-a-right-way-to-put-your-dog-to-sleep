  	    "@context": "http://schema.org",
  	    "@type": "NewsArticle",
  	    "headline": "There\u0027s a Right Way to Put Your Dog to Sleep",
  	    "alternativeHeadline": "",
  	    "url": "http://gq.com/story/theres-a-right-way-to-put-your-dog-to-sleep",
