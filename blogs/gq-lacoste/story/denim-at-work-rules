  	    "alternativeHeadline": "",
  	    "url": "http://gq.com/story/denim-at-work-rules",
  	    "image": ["http://media.gq.com/photos/5632898a56741a6d0871d34d/master/pass/wearing-denim-to-work.jpeg"],
