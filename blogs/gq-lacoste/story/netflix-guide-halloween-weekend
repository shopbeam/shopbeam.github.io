  	    "@context": "http://schema.org",
  	    "@type": "NewsArticle",
  	    "headline": "Your Netflix Guide to Avoiding Halloween Like a Pro",
  	    "alternativeHeadline": "",
  	    "url": "http://gq.com/story/netflix-guide-halloween-weekend",
  	    "image": ["http://media.gq.com/photos/5633b44bf006d32a02e82a78/master/pass/netflix-and-halloween.jpg"],
