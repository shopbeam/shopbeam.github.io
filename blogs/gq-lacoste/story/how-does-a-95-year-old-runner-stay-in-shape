  	    "image": ["http://media.gq.com/photos/55e76199e4fe554f30881dd1/master/pass/GeorgeEntzweiler-1.jpg"],
  	    "thumbnailUrl": "http://media.gq.com/photos/55e76199e4fe554f30881dd1/master/pass/GeorgeEntzweiler-1.jpg",
  	    "datePublished": "2015-09-03T10:00:00.000Z",
  	    "dateModified": "2015-09-02T21:56:12.662Z",
