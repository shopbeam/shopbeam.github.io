  	    "@context": "http://schema.org",
  	    "@type": "NewsArticle",
  	    "headline": "Drone Hacks! Funny Things to Try With Your New Drone",
  	    "alternativeHeadline": "",
  	    "url": "http://gq.com/story/drone-cool-hacks",
  	    "image": ["http://media.gq.com/photos/5582c105e52bc4b477a9bb4d/master/pass/entertainment-2015-06-train-your-drone-96.jpg"],
  	    "thumbnailUrl": "http://media.gq.com/photos/5582c105e52bc4b477a9bb4d/master/pass/entertainment-2015-06-train-your-drone-96.jpg",
