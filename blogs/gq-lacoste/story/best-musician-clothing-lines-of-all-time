  	    "@type": "NewsArticle",
  	    "headline": "A Short and Funky History of Musicians Making Cool Clothes",
  	    "alternativeHeadline": "",
  	    "url": "http://gq.com/story/best-musician-clothing-lines-of-all-time",
  	    "image": ["http://media.gq.com/photos/5608b9f9fbd4424519abfb41/master/pass/apple-boutique-the-beatles.jpg"],
