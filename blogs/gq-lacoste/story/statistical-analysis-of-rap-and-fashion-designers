  	    "@context": "http://schema.org",
  	    "@type": "NewsArticle",
  	    "headline": "A Statistical Analysis of the Most Popular Fashion Designers in Rap",
  	    "alternativeHeadline": "A fun little GQ experiment: Could we figure out who the most popular designers were based on song mentions?",
  	    "url": "http://gq.com/story/statistical-analysis-of-rap-and-fashion-designers",
  	    "image": ["http://media.gq.com/photos/55a954b14e6e4c4f31a484dc/master/pass/Meek-Mill-Main-Pub-2-James-Dimmock.jpg"],
