  	    "image": ["http://media.gq.com/photos/562123faa7113fb922cd7430/master/pass/david-chang-gq-1115-01.jpg"],
  	    "thumbnailUrl": "http://media.gq.com/photos/562123faa7113fb922cd7430/master/pass/david-chang-gq-1115-01.jpg",
